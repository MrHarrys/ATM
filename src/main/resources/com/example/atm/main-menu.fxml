<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="180.0" prefWidth="200.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.atm.MainMenu">
<padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
</padding>
    <children>
        <Button fx:id="sendButton" layoutY="105.0" mnemonicParsing="false" onAction="#onSendButtonClicked" prefWidth="60.0" text="Send" />
        <Button fx:id="charityButton" layoutY="141.0" mnemonicParsing="false" onAction="#onCharityButtonClicked" prefWidth="60.0" text="Charity" />
        <Button fx:id="balanceButton" layoutX="140.0" layoutY="105.0" mnemonicParsing="false" onAction="#onBalanceButtonClicked" prefWidth="60.0" text="Balance" />
        <Button fx:id="exitButton" layoutX="140.0" layoutY="141.0" mnemonicParsing="false" onAction="#onExitButtonClicked" prefWidth="60.0" text="Exit" />
        <Label fx:id="greetingText" layoutX="14.0" layoutY="14.0" text="" />
    </children>
</VBox>
